grid = [line for line in input().lines()];
H, W = [grid.len(), grid[0].len()];

slopes = [[1, 1], [3, 1], [5, 1], [7, 1], [1, 2]];

fn count_slope(slope) {
  dx, dy = slope;
  px, py, trees = [0, 0, 0];
  while py < H {
      trees += 1 if grid[py][px] == "#";
      px = (px + dx) % W;
      py += dy;
  };
  trees
};

part1 = count_slope([3, 1]);
part2 = mul([count_slope(slope) for slope in slopes]);

print("Part 1: " + part1);
print("Part 2: " + part2);
