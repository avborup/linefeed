inp = input().lines();
n = 6;

rep = [];
for i in inp {
  rep.append(int(i));
};

part1 = "?";
part2 = "?";

for i in 0..n {
  for j in i+1..n {
    part1 = rep[i] * rep[j] if rep[i] + rep[j] == 2020;
    for k in j+1..n {
      part2 = rep[i] * rep[j] * rep[k] if rep[i] + rep[j] + rep[k] == 2020;
    };
  };
};

print("Part 1: " + part1);
print("Part 2: " + part2);
