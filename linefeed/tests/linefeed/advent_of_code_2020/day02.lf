part1, part2 = 0, 0;

for min, max, char, password in input().find_all(r/(\d+)-(\d+) (\w): (\w+)/n) {
  part1 += 1 if password.count(char) in (min..=max);

  fn check(i) i <= password.len() and password[i-1] == char;
  part2 += 1 if check(min) xor check(max);
};

print("Part 1: " + part1);
print("Part 2: " + part2);
