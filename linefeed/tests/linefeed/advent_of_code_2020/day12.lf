moves = [line.find(r/(\w)(\d+)/n) for line in input().lines()];

pos, dir = (0, 0), (1, 0);
for action, amount in moves {
  match action {
    "N" => pos += (0, amount),
    "S" => pos -= (0, amount),
    "E" => pos += (amount, 0),
    "W" => pos -= (amount, 0),
    "L" => { dir = dir.rot(-amount / 90) },
    "R" => { dir = dir.rot(amount / 90) },
    "F" => pos += amount * dir
  };
};

part1 = sum([abs(x) for x in pos]);
part2 = "?";

print("Part 1:", part1);
print("Part 2:", part2);
