part1, part2 = 0, 0;
for group in [b.lines() for b in input().split("\n\n")] {
  union, intersection = set(), set(group[0]);

  for line in group {
    union += set(line);
    intersection &= set(line);
  };

  part1 += union.len();
  part2 += intersection.len();
};

print("Part 1: " + part1);
print("Part 2: " + part2);
