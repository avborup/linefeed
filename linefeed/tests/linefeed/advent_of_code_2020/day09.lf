nums = [int(line) for line in input().lines()];
preamble = if nums.len() > 25 { 25 } else { 5 };

fn has_two_sum(i) {
  for j in i-preamble..i {
    for k in j+1..i {
      return true if nums[j] + nums[k] == nums[i];
    };
  };
  false
};

part1 = [nums[i] for i in preamble..nums.len() if not (has_two_sum(i))][0];

left, right, sum = 0, 1, sum(nums[0..=1]);
while sum != part1 and right < nums.len() {
  if sum < part1 {
    right += 1;
    sum += nums[right];
  } else {
    sum -= nums[left];
    left += 1;
  };
};

part2 = min(nums[left..=right]) + max(nums[left..=right]);

print("Part 1:", part1);
print("Part 2:", part2);
