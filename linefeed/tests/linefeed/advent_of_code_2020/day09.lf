nums = [int(line) for line in input().lines()];
delta = if nums.len() > 25 { 25 } else { 5 };

fn validate(i) {
  for j in i-delta..i {
    for k in j+1..i {
      if nums[j] + nums[k] == nums[i] {
        return true;
      };
    };
  };
  false
};

for i in delta..nums.len() {
  if not (validate(i)) {
    part1 = nums[i];
    break;
  };
};

left, right, sum = 0, 1, sum(nums[0..=1]);
while sum != part1 and right < nums.len() {
  if sum < part1 {
    right += 1;
    sum += nums[right];
  } else {
    sum -= nums[left];
    left += 1;
  };
};

contiguous = nums[left..=right];
part2 = min(contiguous) + max(contiguous);

print("Part 1:", part1);
print("Part 2:", part2);
